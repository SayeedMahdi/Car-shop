                    <div class="single-ticket-price">
                        <div class="part-feature">
                            <span class="price">{{this.Price}}</span>
                            <h3>Basic Pass</h3>
                            <ul>
                                <li><span class="icon"><i class="fas fa-check"></i></span><span
                                        class="text">Title:{{this.Title}}
                                    </span></li>
                                <li><span class="icon"><i class="fas fa-check"></i></span><span class="text">Daily
                                        Rate:{{this.DailyRate}}
                                    </span></li>
                                <li><span class="icon"><i class="fas fa-check"></i></span><span class="text">Number in
                                        Stuck:{{this.NumberInStock}}
                                    </span></li>
                                <li><span class="icon"><i class="fas fa-check"></i></span><span
                                        class="text">ID:{{this.id}}
