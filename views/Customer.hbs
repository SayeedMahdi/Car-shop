                        <form action="/api/customers" method="POST">
                            <div class="row ">
                                <div class="col-xl-6 col-lg-6 form-group">
                                    <input type="text" placeholder="Name" name="name" class="form-control ">
                                    <span></span>
                                </div>
                                <div class="col-xl-6 col-lg-6 form-group">
                                    <input type="text" placeholder="phone" name="phone" class="form-control ">
                                    <span></span>
                                </div>
                                <div class="col-xl-6 col-lg-6 form-group">
                                    <label for="cars" style="padding-top: 8px;">Is Gold</label>
                                    <select id="cars" name="isGold"
                                        style="background-color: blueviolet; margine-left: 3px;">
                                        <option value="true" style="background-color: blueviolet;">True</option>
                                        <option value="false" style="background-color: blueviolet;">False</option>
                                    </select>
                                    <span></span>
                                </div>
                                <div class="col-xl-6 col-lg-6 form-group">
                                    <input type="text" placeholder="Customer picture" name="photo"
                                        class="form-control ">
                                    <span></span>
                                </div>

                                <div class="col-xl-6 col-lg-6 form-group">
                                    {{#if message}}
                                    <div>
                                        <h4 class="alert alert-danger">
                                            {{message}}
                                        </h4>
                                    </div>
                                    {{/if}}
                                    <button type="submit"> Submit the customer</button>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>

