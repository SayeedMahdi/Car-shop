                    <th scope="col">Phone: </th>
                    <th scope="col">isGold: </th>
                    <th scope="col">Action</th>
                </tr>
            </thead>
            <tbody>
                {{#each row}}
                <tr>
                    <th scope="row"> {{this.id}}</th>
                    <td>{{this.name}}</td>
                    <td>{{this.phone}}</td>
                    <td>1</td>
